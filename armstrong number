public class main{
public sttatic void main(string args[]){
int n , args, sum =0, r;
n =153;
arg = n;
for(int  i =1; i<n;i++){
while(n>0){
r=n%10;
sum = sum+(r*r*r);
n=n/10;
}
}
if(arg==sum){
system.out.println("given num is armstrong:"+ arg);
else{
system.out.println("given num is not armstrong:"+ arg);
}
}
}
}
